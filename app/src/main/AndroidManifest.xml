    package="au.edu.unsw.infs3634.assignmentui">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="Fun Study"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ChooseLanguage" />
        <activity android:name=".QuizPage" />
        <activity android:name=".GuideActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".LessonActivity"/>
        <activity android:name=".StudyActivity"/>
        <activity android:name=".ExamActivity"/>
        <activity android:name=".api.JokeActivity"/>
